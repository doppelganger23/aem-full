<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Create navigation pages underneath it"
    jcr:primaryType="cq:Template"
    jcr:title="sapustraining Home Page"
    allowedPaths="[/content/sapustraining(/.*)?]"
    ranking="{Long}100">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="sapustraining/components/structure/page-content">
        <sap-navbar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sapustraining/components/structure/sap-navbar">
            <sap-logo
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="sapustraining/components/content/sap-logo"
                    logoLinkText="Developers"
                    logoLinkUrl="#"/>
            <sap-nav-list
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="sapustraining/components/content/sap-nav-list"/>
        </sap-navbar>
        <par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <image-block
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/apps/sapustraining/components/content/background-image-block">
            </image-block>
        </par>
        <par2
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/parsys">
            <three-component-container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="/apps/sapustraining/components/content/three-component-container">
                <par-1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys">
                    <feature
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/apps/sapustraining/components/content/textimage-feature">
                    </feature>
                </par-1>
                <par-2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys">
                    <feature
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/apps/sapustraining/components/content/textimage-feature">
                    </feature>
                </par-2>
                <par-3
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys">
                    <feature
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/apps/sapustraining/components/content/textimage-feature">
                    </feature>
                </par-3>
            </three-component-container>
            <video-container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="/apps/sapustraining/components/content/video-block">
            </video-container>
        </par2>
        <sap-footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="sapustraining/components/structure/sap-footer">
            <par-f1
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/iparsys">
                <four-components-container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/apps/sapustraining/components/content/four-component-container">
                    <par-1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/parsys">
                        <footer-multilink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/apps/sapustraining/components/content/multi-link">
                        </footer-multilink>
                    </par-1>
                    <par-2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/parsys">
                        <footer-multilink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/apps/sapustraining/components/content/multi-link">
                        </footer-multilink>
                    </par-2>
                    <par-3
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/parsys">
                        <footer-multilink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/apps/sapustraining/components/content/multi-link">
                        </footer-multilink>
                    </par-3>
                    <par-4
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/parsys">
                        <footer-multilink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/apps/sapustraining/components/content/multi-link">
                        </footer-multilink>
                    </par-4>
                </four-components-container>
            </par-f1>
            <par-f2
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/iparsys">
            </par-f2>
        </sap-footer>
    </jcr:content>
</jcr:root>
